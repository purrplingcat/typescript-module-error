{
  "name": "senses",
  "packageManager": "yarn@3.2.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "bin": {
    "senses": "bin/senses.js",
    "senses-ui": "bin/senses-ui.js"
  },
  "scripts": {
    "build": "tsc --build packages/core packages/roztoky"
  },
  "dependencies": {
    "@senses/core": "workspace:^",
    "@senses/roztoky": "workspace:^",
    "date-fns": "^2.28.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.40",
    "typescript": "^4.7.3"
  }
}
